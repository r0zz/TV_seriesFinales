<p id="notice"><%= notice %></p>

<h1>Myusers</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>Email</th>
      <th>Watched</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myusers.each do |myuser| %>
      <tr>
        <td><%= myuser.username %></td>
        <td><%= myuser.password %></td>
        <td><%= myuser.email %></td>
        <td><%= myuser.watched %></td>
        <td><%= link_to 'Show', myuser %></td>
        <td><%= link_to 'Edit', edit_myuser_path(myuser) %></td>
        <td><%= link_to 'Destroy', myuser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myuser', new_myuser_path %>
